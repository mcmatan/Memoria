EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/KontaktSDK" $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/KontaktSDK"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/KontaktSDK" $(inherited) "${PODS_ROOT}/KontaktSDK/KontaktSDK"
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Realm.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RealmSwift.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SnapKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Swinject.framework/Headers" -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/KontaktSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"kontakt-ios-sdk" -framework "CoreBluetooth" -framework "CoreLocation" -framework "Foundation" -framework "MobileCoreServices" -framework "Realm" -framework "RealmSwift" -framework "SnapKit" -framework "Swinject" -framework "SystemConfiguration" -framework "UIKit"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Memoria
PODS_ROOT = ${SRCROOT}/Pods